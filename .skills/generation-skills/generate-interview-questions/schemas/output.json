{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Generate Interview Questions Output",
  "description": "Output schema for the generate-interview-questions skill",
  "type": "object",
  "properties": {
    "status": {
      "type": "string",
      "enum": ["success", "review", "failed", "budget_exceeded"]
    },
    "output": {
      "type": "object",
      "properties": {
        "questions": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": {"type": "integer"},
              "category": {
                "type": "string",
                "enum": ["strategy", "audience", "voice", "competitive", "content", "founder", "metrics", "validation", "general"]
              },
              "question": {"type": "string"},
              "purpose": {"type": "string"},
              "gap_addressed": {"type": ["string", "null"]},
              "assumption_validated": {"type": "string"},
              "priority": {"type": "string", "enum": ["high", "medium", "low"]}
            },
            "required": ["id", "question"]
          }
        },
        "research_gaps": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "area": {"type": "string"},
              "description": {"type": "string"}
            }
          }
        },
        "assumptions_to_validate": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "assumption": {"type": "string"},
              "source": {"type": "string"}
            }
          }
        },
        "interview_guide": {"type": "string"},
        "interview_guide_path": {"type": "string"}
      }
    },
    "evaluation": {"type": "object"},
    "release_action": {"type": "string"},
    "run_id": {"type": "string"},
    "_meta": {
      "type": "object",
      "properties": {
        "client_id": {"type": "string"},
        "tenant_id": {"type": "string"},
        "run_id": {"type": "string"},
        "research_docs_loaded": {"type": "integer"},
        "runtime_seconds": {"type": "number"},
        "cost_usd": {"type": "number"},
        "timestamp": {"type": "string"}
      }
    },
    "error": {"type": "string"},
    "error_type": {"type": "string"}
  },
  "required": ["status"]
}
